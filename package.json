{
  "name": "vitepress-plugin-group-icons",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "pnpm@9.7.0",
  "author": "Leo <https://github.com/yuyinws>",
  "license": "MIT",
  "homepage": "https://github.com/yuyinws/vitepress-plugin-group-icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyinws/vitepress-plugin-group-icons.git"
  },
  "bugs": "https://github.com/yuyinws/vitepress-plugin-group-icons/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src/client"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vitepress": "^1.0.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@iconify/utils": "^2.1.30",
    "@iconify/vue": "^4.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@antfu/ni": "^0.22.0",
    "@antfu/utils": "^0.7.10",
    "@iconify-json/logos": "^1.1.44",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.14.11",
    "bumpp": "^9.4.1",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.7",
    "markdown-it": "^14.1.0",
    "pnpm": "^9.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "3.0.0-rc.7",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
